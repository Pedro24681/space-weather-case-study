{"cells": [{"cell_type": "code", "metadata": {}, "source": ["# Cell 1 - Imports and Data Loading\nimport pandas as pd\nimport numpy as np\nimport seaborn as sns\nimport matplotlib.pyplot as plt\nimport statsmodels.api as sm\n\n# Load Data\ndata = pd.read_csv('data.csv')\n"]}, {"cell_type": "code", "metadata": {}, "source": ["# Cell 2 - Correlation Matrix Heatmap\ncorr = data.corr()\nsns.heatmap(corr, annot=True, cmap='coolwarm')\nplt.title('Correlation Matrix')\nplt.show()\n"]}, {"cell_type": "code", "metadata": {}, "source": ["# Cell 3 - Lag Correlation Analysis\nfor lag in range(1, 11):\n    lag_correlation = data['variable'].autocorr(lag=lag)\n    print(f'Lag {lag}: {lag_correlation}')\n"]}, {"cell_type": "code", "metadata": {}, "source": ["# Cell 4 - Cross-Correlation Plots\nsm.tsa.graphics.tsa_plot(data['variable1'], data['variable2'], lags=40)\nplt.show()\n"]}, {"cell_type": "code", "metadata": {}, "source": ["# Cell 5 - Event Detection Algorithm with Threshold\ndef detect_events(data, threshold):\n    return data[data['value'] > threshold]\n\ndetected_events = detect_events(data, threshold=10)\ndetected_events\n"]}, {"cell_type": "code", "metadata": {}, "source": ["# Cell 6 - Storm Catalog Generation\nstorm_catalog = detected_events.groupby('event_id').agg({'timestamp': 'min', 'value': 'max'})\nstorm_catalog\n"]}, {"cell_type": "code", "metadata": {}, "source": ["# Cell 7 - Statistical Significance Testing with P-Values\nfrom scipy import stats\np_values = stats.norm.sf(abs(storm_catalog['value'])) * 2\np_values\n"]}, {"cell_type": "code", "metadata": {}, "source": ["# Cell 8 - Visualization of Detected Events\nplt.figure(figsize=(12, 6))\nplt.plot(data['timestamp'], data['value'], label='Value')\nplt.scatter(detected_events['timestamp'], detected_events['value'], color='red', label='Detected Events')\nplt.title('Detected Events')\nplt.legend()\nplt.show()\n"]}, {"cell_type": "code", "metadata": {}, "source": ["# Cell 9 - Summary Statistics\nsummary_statistics = data.describe()\nsummary_statistics\n"]}]}